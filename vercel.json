{
  "version": 2,
  "builds": [
    {
      "src": "api/auth.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/chat.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/auth/(.*)", "dest": "api/auth.js" },
    { "src": "/api/chat/(.*)", "dest": "api/chat.js" },
    { "src": "/(.*)", "dest": "api/index.js" }
  ]
}